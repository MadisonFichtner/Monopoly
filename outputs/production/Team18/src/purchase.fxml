<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox nodeOrientation="LEFT_TO_RIGHT" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.PurchaseController">
   <children>
      <Label fx:id="deedLabel" alignment="TOP_CENTER" />
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="yesButton" mnemonicParsing="false" text="Yes" />
            <Button fx:id="noButton" mnemonicParsing="false" text="No" />
         </children>
      </HBox>
   </children>
</VBox>
